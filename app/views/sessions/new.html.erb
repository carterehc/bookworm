<%provide(:title, 'Login')%>
<%=render 'layouts/home_nav' %>

<h1>Login</h1>
<div class = 'row'>
    <div class='center'>
        <%= form_for(:session, url: login_path) do |f|%>
            
            
            <%= f.label :username %>
            <%= f.text_field :username, class: 'form_control'%>
            
            <%= f.label :password %>
            <%= f.password_field :password, class: 'form_control' %>
            <%= f.submit 'Login', class: 'btn btn-primary' %>
        <%end%>
        <p>Don't have an account? <%= link_to 'Sign Up', signup_path %></p>
    </div>
</div>