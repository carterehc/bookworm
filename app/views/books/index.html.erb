<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>
<% @page_title = "My Books"%>
<div class="table-responsive">
    <table class="table">
        <thead>
            <tr>
                <th scope = "col">Title</th>
                <th scope = "col">Author First Name</th>
                <th scope = "col">Author Last Name</th>
                <th scope = "col">Published</th>
                <th scope = "col">Read</th>
                <th scope = "col">Year Read</th>
                <th scope = "col">Times Read</th>
                <th scope = "col">Own</th>
                <th scope="col">ISBN</th>
                <th scope = "col">Tag</th>
            
            </tr>
        </thead>
        <tbody>
            <% @books.each do |book|%>
            <tr>
                <th scope="row"></th>
                    <td>book.title</td>
                    <td>book.author_id.fName</td>
                    <td>book.author_id.lName</td>
                    <td>book.yr_published
            </tr>
            <% end%>
        </tbody>
    </table>
</div>